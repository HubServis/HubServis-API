{
    "openapi": "3.0.1",
    "info": {
        "title": "HubServis",
        "description": "Documentation the HubServis api",
        "version": "1.0.0"
    },
    "basePath": "/",
    "paths": {
        "/login": {
            "post": {
                "requestBody": {
                    "content": {
                        "aplication/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Loged"
                    }
                }
            }
        },
        "/user": {
            "post": {
                "requestBody": {
                    "content": {
                        "aplication/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "email": {
                                        "type": "string"
                                    },
                                    "cpfcnpj": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "User data created and token"
                    }
                }
            }
        }
    }
}